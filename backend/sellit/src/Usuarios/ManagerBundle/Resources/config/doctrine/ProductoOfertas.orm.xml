<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios\ManagerBundle\Entity\ProductoOfertas" table="producto_ofertas">
    <indexes>
      <index name="producto_oferta_fk_idx" columns="id_producto"/>
      <index name="usuario_oferta_fk" columns="id_usuario"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="oferta" type="float" column="oferta" precision="10" scale="0" nullable="false"/>
    <field name="comentarios" type="text" column="comentarios" nullable="true"/>
    <field name="estadoOferta" type="string" column="estado_oferta" length="45" nullable="true"/>
    <many-to-one field="idUsuario" target-entity="Usuarios">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProducto" target-entity="Producto">
      <join-columns>
        <join-column name="id_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
