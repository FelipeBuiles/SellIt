<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios\ManagerBundle\Entity\Usuarios" table="usuarios">
    <unique-constraints>
      <unique-constraint name="ID_UNIQUE" columns="id_front"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idFront" type="string" column="id_front" length="255" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false"/>
    <field name="fechaRegistro" type="datetime" column="fecha_registro" nullable="true"/>
    <field name="rutaAvatar" type="text" column="ruta_avatar" nullable="true"/>
    <field name="latitud" type="float" column="latitud" precision="10" scale="0" nullable="false"/>
    <field name="longitud" type="float" column="longitud" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
