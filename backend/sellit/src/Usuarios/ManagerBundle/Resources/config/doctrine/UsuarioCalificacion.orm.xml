<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios\ManagerBundle\Entity\UsuarioCalificacion" table="usuario_calificacion">
    <indexes>
      <index name="usuario_fk_idx" columns="id_usuario"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="calificacion" type="integer" column="calificacion" nullable="false"/>
    <field name="comentario" type="text" column="comentario" nullable="true"/>
    <many-to-one field="idUsuario" target-entity="Usuarios">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
