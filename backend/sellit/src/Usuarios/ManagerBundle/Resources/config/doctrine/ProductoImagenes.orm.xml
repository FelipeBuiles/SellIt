<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios\ManagerBundle\Entity\ProductoImagenes" table="producto_imagenes">
    <indexes>
      <index name="producto_fk_idx" columns="id_producto"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rutaImagen" type="string" column="ruta_imagen" length="255" nullable="false"/>
    <field name="nombreImagen" type="string" column="nombre_imagen" length="255" nullable="false"/>
    <many-to-one field="idProducto" target-entity="Producto">
      <join-columns>
        <join-column name="id_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
